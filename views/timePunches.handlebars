<div class="row">
    <div class="col m12">
        <div class="card blue-grey darken-1">
            <div class="card-content center-align black-text white">
                <span class="card-title">Current Time Punches</span>
            </div>
            <div class="card-content center-align black-text white">
                <table class="highlight">
                    <thead>
                        <tr>
                            <th>Date</th>
                            <th>Punch In</th>
                            <th>Punch Out</th>
                            <th class="right-align">View Location</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each TimePunch}}
                        <tr>
                            <td>{{clockDate}}</td>
                            <td>{{clockIn}}</td>
                            <td>{{clockOut}}</td>
                            <td>
                                <!-- Modal Map Trigger -->
                                <a class="btn-floating btn-small waves-effect waves-light red modal-trigger"
                                    href="#mapModal"><i class="material-icons">location_on</i></a>
                            </td>
                            <!-- Modal Map Structure -->
                                                   <div id="mapModal" class="modal">
                            <div class="modal-content">
                                <h4>Location</h4>
                                <p>timePunches</p>
                                <div id="map">MAP</div>
                            </div>
                            <script id="googleMaps">
                                var map;
                                function initMap() {
                                    map = new google.maps.Map(document.getElementById('map'), {
                                        center: { lat: {{empLat}}, lng: {{empLon}} },
                                zoom: 17
        });
      }
                            </script>
                            {{!-- <script src=“https://maps.googleapis.com/maps/api/js?key=“+process.env.MAPAPI+“&callback=initMap” async defer></script> --}}
                                <div class="modal-footer">
                                    <a href="#!" class="modal-close waves-effect waves-green btn-flat">Close</a>
                                </div>
                            </div>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
</div>