<div class="row">
    <div class="col m12">
        <div class="card blue-grey darken-1">
            <div class="card-content center-align black-text white">
                <span class="card-title">Employee List</span>
            </div>
            <div class="card-content center-align black-text white">
                <table class="highlight responsive-table">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Employee Name</th>
                            <th class="right-align">Actions</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each employee}}
                        <tr>
                            <td>{{userId}}</td>
                            <td>{{empName}}</td>
                            <td class="right-align"> <a data-id="{{id}}" class="fired btn-floating btn-small waves-effect waves-light red"><i
                                        class="material-icons">delete_forever</i></a>&nbsp;&nbsp; <a
                                    class="btn-floating btn-small waves-effect waves-light red" href="/timePunches"><i
                                        class="material-icons">visibility</i></a> </td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
</div>